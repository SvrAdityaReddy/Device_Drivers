[38;5;121mstatic[m [38;5;121mvoid[m safe_udelay([38;5;121munsigned[m [38;5;121mlong[m usecs)
{[27;9H[93mwhile[m (usecs > MAX_UDELAY_US) {[28;17Hudelay(MAX_UDELAY_US);[29;17Husecs -= MAX_UDELAY_US;[30;9H}[31;9Hudelay(usecs);
