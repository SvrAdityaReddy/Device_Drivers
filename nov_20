    [93mfor[m (i = [95m0[m; i < [95m67[m; i++) {[6;17H[93mif[m (i%[95m2[m)[7;25Hsend_space(l[i] - delta);[8;17H[93melse[m[9;25Hdelta = send_pulse(l[i]);[10;9H}[11;9Hgpiochip->set(gpiochip, gpio_out_pin, invert);[13;9Hspin_unlock_irqrestore(&lock, flags);[14;9H[93mreturn[m;
}

[38;5;121mstatic[m [38;5;121mvoid[m send_hex_code([38;5;121mchar[m *val)
